@{
    ViewBag.Title = "T13054";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Apps/JSController/Transaction/T13054Controller.js"></script>
<script src="~/Apps/JSService/Transaction/T13054Service.js"></script>

@section container
{
<div id="options" ng-app="BloodBank" ng-controller="T13054Controller">
    <button id="btnNew" title="New" class="Button New"></button>
    <button id="btnSave" title="Save" class="Button Save" ng-disabled="obj.myDiv.$invalid"></button>
    <button id="btnNext" title="Next" class="Button Next"></button>
    <button id="btnDelete" title="Delete" class="Button Delete"></button>
    <button id="btnClear" title="Clear" class="Button Clear" ng-click="onClear()"></button>
    <button id="btnEnterQuery" title="Enter Query" class="Button Enter"></button>
    <button id="btnExecuteQuery" title="Execute Query" class="Button Execute"></button>
    <button id="btnPrint" title="Print" class="Button Print"></button>
</div>
}
<div class="form-horizontal" ng-app="BloodBank" ng-controller="T13054Controller" name="obj.myDiv" novalidate ng-form>
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12" style="border: 1px solid #c12d2d;margin-top: 10px; border-radius: 5px;padding: 5px 0 5px 0;">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-1 col-md-1 col-sm-1 "></div>
                    <div class="col-lg-1 col-md-1 col-sm-1 " style="text-align: right">
                        <label id="lblModelId" for="txtModelId" class="ng-cloak" ng-bind="getLabel('lblModelId')"></label>
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-2 ">
                        <input id="txtModelId" ng-model="obj.T13054.T_MODEL_CODE" ng-dblclick="openModel()" ng-keydown="openModelPopup($event)" type="text" style="width: 100%" />
                    </div>
                    <div class="col-lg-5 col-md-5 col-sm-5 "><input id="txtModelDesc" type="text" ng-model="obj.T13054.ANALYZER_MODEL" style="width: 100%" readonly="readonly"/></div>
                    <div class="col-lg-2 col-md-2 col-sm-2 ">
                        <input type="button" value="{{getLabel('btnInsertCorelation')}}" id="btnInsertCorelation" style="width: 100%; background: #c12d2d; color: #ffffff"  class="ng-cloak" />
                    </div>
                    <div class="col-lg-1 col-md-1 col-sm-1 "></div>
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-2 col-md-2 col-sm-2 " style="text-align: right">
                        <label id="lblDateFrom" for="txtDateFrom" class="ng-cloak" ng-bind="getLabel('lblDateFrom')"></label>
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-2 ">
                        <datepicker date-format="dd/MM/yyyy">
                            <input id="txtDateFrom" ng-model="obj.T_LOT_EXPIRYdd" type="text" readonly="readonly" style="cursor: pointer; width: 100%" />
                        </datepicker>
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-2 "></div>
                    <div class="col-lg-1 col-md-1 col-sm-1 " style="text-align: right">
                        <label id="lblDateTo" for="txtDateTo" class="ng-cloak" ng-bind="getLabel('lblDateTo')"></label>
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-2 ">
                        <datepicker date-format="dd/MM/yyyy">
                            <input id="txtDateTo" ng-model="obj.T_LOT_EXPIRY" type="text" readonly="readonly" style="cursor: pointer; width: 100%"/>
                        </datepicker>
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-2 ">
                        <input type="button" value="{{getLabel('btnModelDelete')}}" id="btnModelDelete" style="width: 100%; background: #c12d2d; color: #ffffff" class="ng-cloak"/>
                    </div>
                    <div class="col-lg-1 col-md-1 col-sm-1 "></div>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12" style="border: 1px solid #c12d2d; margin-top: 10px; height: 380px; border-radius: 5px;padding: 5px 0 5px 0;">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-3 col-md-3 col-sm-3" style="text-align: center">
                        <label id="lblSampleId" class="ng-cloak" style="color:#3A7D2F" ng-bind="getLabel('lblSampleId')"></label>
                    </div>
                    <div class="col-lg-9 col-md-9 col-sm-9"  style="text-align: center">
                        <label id="lblResultDetail" class="ng-cloak" style="color:#3A7D2F" ng-bind="getLabel('lblResultDetail')"></label>
                    </div>
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <table class="table table-bordered" style="width: 100%; margin: 5px auto; border: 1px solid #c12d2d; border-radius: 5px;">
                            <thead style="text-align: center; display: table; width: 100%;">
                            <tr style="background-color: #c12d2d; color: #ffffff">
                                <th id="lblVerify" style="text-align: center;width: 4%" class="ng-cloak" ng-bind="getLabel('lblVerify')"></th>
                                <th id="lblBarcodeNo" style="text-align: center; width: 9%" class="ng-cloak" ng-bind="getLabel('lblBarcodeNo')"></th>
                                <th id="lblResultDate" style="text-align: center; width: 9%" class="ng-cloak" ng-bind="getLabel('lblResultDate')"></th>
                                <th id="lblAnalysis" style="text-align: center; width: 20%" class="ng-cloak" ng-bind="getLabel('lblAnalysis')"></th>
                                <th id="lblResult" style="text-align: center; width: 5%" class="ng-cloak" ng-bind="getLabel('lblResult')"></th>
                                <th id="lblResultDescription" style="text-align: center;width: 15%" class="ng-cloak" ng-bind="getLabel('lblResultDescription')"></th>
                                <th id="lblFrom" style="text-align: center;width: 4%" class="ng-cloak" ng-bind="getLabel('lblFrom')"></th>
                                <th id="lblTo" style="text-align: center;width: 3%" class="ng-cloak" ng-bind="getLabel('lblTo')"></th>
                                <th id="lblFlag" style="text-align: center;width: 4%" class="ng-cloak" ng-bind="getLabel('lblFlag')"></th>
                                <th id="lblResult_Notes" style="text-align: center;width: 19%" class="ng-cloak" ng-bind="getLabel('lblResult_Notes')"></th>
                                <th id="lblResFile" style="text-align: center;width: 7%" class="ng-cloak" ng-bind="getLabel('lblResFile')"></th>

                            </tr>
                            </thead>
                            <tbody style="text-align: center; width: 100%;max-height:250px; min-height:250px; overflow:auto; overflow-x:hidden; display:block;">
                            <tr ng-repeat="obj in obj.TestList" style="display: table; width: 100%;height: 25px;" >
                                <td style="width: 4%"></td>
                                <td style="width: 9%"></td>
                                <td style="width: 9%"></td>
                                <td style="width: 20%"></td>
                                <td style="width: 5%"></td>
                                <td style="width: 15%"></td>
                                <td style="width: 4%"></td>
                                <td style="width: 3%"></td>
                                <td style="width: 4%"></td>
                                <td style="width: 19%"></td>
                                <td style="width: 7%"></td>
                            </tr>
                                
                               
                            </tbody>

                        </table>
                    </div>
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-2 col-md-2 col-sm-2" style="text-align: right">
                            <label id="lblPatName" for="txtPatName" ng-bind="getLabel('lblPatName')" class="ng-cloak"></label>
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <input id="txtPatName" type="text" style="width: 100%" readonly="readonly"/>
                        </div>
                        <div class="col-lg-1 col-md-1 col-sm-1" style="text-align: right">
                            <label id="lblGender" for="txtGender" class="ng-cloak" ng-bind="getLabel('lblGender')"></label>
                        </div>
                        <div class="col-lg-1 col-md-1 col-sm-1">
                            <input id="txtGender" type="text" style="width: 100%" readonly="readonly"/>
                        </div>
                        <div class="col-lg-2 col-md-2 col-sm-2">
                            <label id="lblYrs" for="txtYrs" class="ng-cloak" ng-bind="getLabel('lblYrs')"></label>
                            <input id="txtYrs" type="text" style="width: 25%" readonly="readonly"/>
                            <input id="txtMos" type="text" style="width: 25%" readonly="readonly"/>
                            <label id="lblMos" for="txtMos" class="ng-cloak"  ng-bind="getLabel('lblMos')"></label>
                        </div>
                        <div class="col-lg-2 col-md-2 col-sm-2">
                          
                        </div>
                    </div>
                </div>

            </div>
            
        </div>
        <div id="divModel" class="omf-modal">
            <div class="omf-modal-body">
                <div class="omf-modal-header">
                    <label id="lblModelHeader" ng-bind="getLabel('lblModelHeader')" class="ng-cloak omf-modal-header-Label"></label><input type="button" class="omf-modal-header-Close" ng-click="CloseModelPopup()" />
                </div>
                <div class="omf-modal-div-placeholder">
                    <input type="text" placeholder="{{getLabel('lblPlaceHolderSearch')}}" class="omf-modal-placeholder" ng-model="Search" />

                </div>
                <div class="form-horizontal popup">
                    <div class="form-group">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <table class="omf-modal-grid grid">
                                <thead>
                                    <tr>
                                        <th style="width: 20%">
                                            <label id="lblModelCode" ng-bind="getLabel('lblModelCode')" class="ng-cloak"></label>
                                        </th>
                                        <th style="width: 80%">
                                            <label id="lblAnalizarModel" ng-bind="getLabel('lblAnalizarModel')" class="ng-cloak"></label>
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr dir-paginate="obj in obj.ModelList |filter : Search |itemsPerPage:pageSize" current-page="currentPage" pagination-id="model" style="cursor: pointer" ng-click="onRowSelect(PagiIndex($index,currentPage))">
                                        <td style="text-align: center;width: 20%"><span id="spnModelCode{{$index}}" style="width: 100%">{{obj.T_MODEL_CODE}}</span></td>
                                        <td style="text-align: left;width: 80%"><span id="spnAnalyzerModel{{$index}}" style="width: 100%">{{obj.ANALYZER_MODEL}}</span></td>
                                    </tr>

                                </tbody>
                            </table>
                            <dir-pagination-controls pagination-id="model" max-size="5"
                                                     direction-links="true"
                                                     boundary-links="true">
                            </dir-pagination-controls>


                        </div>
                    </div>
                </div>
            </div>
        </div>
       
    </div>
</div>