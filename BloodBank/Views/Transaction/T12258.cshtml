
@{
    ViewBag.Title = "T12258";
}
<script src="~/Apps/JSController/Transaction/T12258Controller.js"></script>
<script src="~/Apps/JSService/Transaction/T12258Service.js"></script>


@section container
{
    <div id="options" ng-app="BloodBank" ng-controller="T12258Controller">
        <button id="btnNew" title="New" class="Button New"></button>
        <button id="btnSave" title="Save" class="Button Save" ng-disabled="obj.myDiv.$invalid"></button>
        <button id="btnNext" title="Next" class="Button Next"></button>
        <button id="btnDelete" title="Delete" class="Button Delete"></button>
        <button id="btnClear" title="Clear" class="Button Clear"></button>
        <button id="btnEnterQuery" title="Enter Query" class="Button Enter"></button>
        <button id="btnExecuteQuery" title="Execute Query" class="Button Execute"></button>
    </div>
}
<div class="form-horizontal" ng-app="BloodBank" ng-controller="T12258Controller" name="obj.myDiv" novalidate ng-form>

    <div class="row" id="1stDiv" style="border: 1px solid #b2aeae; border-radius: 5px; padding: 10px;  margin: 1.6% 14% auto">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div>
                <table style="width: 100%">
                    <thead style="text-align: center">
                        <tr style="color: #29449b; width: 100%">
                            <td style="width:10%"><span id="lblUnit" ng-bind="getLabel('lblUnit')"></span></td>
                            <td style="width:10%"><span id="lblProd" ng-bind="getLabel('lblProd')"></span></td>
                            <td style="width:10%"><span id="lblProduct"  ng-bind="getLabel('lblProduct')"></span></td>
                            <td style="width:10%"><span id="lblBloodGrp"  ng-bind="getLabel('lblBloodGrp')"></span></td>
                            <td style="width:10%"><span id="lblExpDays" ng-bind="getLabel('lblExpDays')"></span></td>
                            <td style="width:12%"><span id="lblDonation" ng-bind="getLabel('lblDonation')"></span></td>
                            <td style="width:5%"><span id="lblSec" ng-bind="getLabel('lblSec')"></span>
                            <td style="width:7%"><span id="lblDos" ng-bind="getLabel('lblDos')"></span>
                            <td style="width:10%"><span id="lblDept" ng-bind="getLabel('lblDept')"></span>
                            <td style="width:10%"><span id="lblEnrty" ng-bind="getLabel('lblEnrty')"></span>
                            <td style="width:6%"><span id="lblTime" ng-bind="getLabel('lblTime')"></span>
                            </td>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="obj in obj.demo" style="width: 100%">
                            <td style="width: 10%">
                                <input type="text" style="width: 100%" class="form-control" readonly />
                            </td>
                            <td style="width: 10%">
                                <input type="text" style="width: 100%" class="form-control" readonly />
                            </td>
                            <td style="width: 10%">
                                <input type="text" style="width: 100%" class="form-control" readonly />
                            </td>
                            <td style="width: 10%">
                                <input type="text" style="width: 100%" class="form-control" readonly />
                            </td>
                            <td style="width: 10%">
                                <input type="text" style="width: 100%" class="form-control" readonly />
                            </td>
                            <td style="width: 12%">
                                <input type="text" style="width: 100%" class="form-control" readonly />
                            </td>
                            <td style="width: 5%">
                                <input type="text" style="width: 100%" class="form-control" readonly />
                            </td>
                            <td style="width: 7%">
                                <input type="text" style="width: 100%" class="form-control" readonly />
                            </td>
                            <td style="width: 10%">
                                <input type="text" style="width: 100%" class="form-control" readonly />
                            </td>
                            <td style="width: 10%">
                                <input type="text" style="width: 100%" class="form-control" readonly />
                            </td>
                            <td style="width: 6%">
                                <input type="text" style="width: 100%" class="form-control" readonly />
                            </td>

                        </tr>
                    </tbody>
                    <tbody>
                        <tr style="width:100%">
                            <td style="width:10%"></td>
                            <td style="width:20%;text-align:right" colspan="2">
                                <span id="lblBlood" style="text-align: center;" ng-bind="getLabel('lblBlood')"></span>
                            </td>
                            <td style="width: 10%">
                                <input type="text" style="width: 100%" class="form-control" readonly />
                            </td>
                            <td style="width: 22%" colspan="2">
                                <input type="text" style="width: 100%" class="form-control" readonly />
                            </td>
                            <td style="width:38%" colspan="5"></td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

