
@{
    ViewBag.Title = "T12253";
}
<script src="~/Apps/JSController/Transaction/T12253Controller.js"></script>
<script src="~/Apps/JSService/Transaction/T12253Service.js"></script>


@section container
{
    <div id="options" ng-app="BloodBank" ng-controller="T12253Controller">
        <button id="btnNew" title="New" class="Button New"></button>
        <button id="btnSave" title="Save" class="Button Save" ng-disabled="obj.myDiv.$invalid"></button>
        <button id="btnNext" title="Next" class="Button Next"></button>
        <button id="btnDelete" title="Delete" class="Button Delete"></button>
        <button id="btnClear" title="Clear" class="Button Clear"></button>
        <button id="btnEnterQuery" title="Enter Query" class="Button Enter"></button>
        <button id="btnExecuteQuery" title="Execute Query" class="Button Execute"></button>
    </div>
}
    <div class="form-horizontal" ng-app="BloodBank" ng-controller="T12253Controller" name="obj.myDiv" novalidate ng-form>
        <div class="row" style="margin: 1.6% auto; border: 1px solid #b2aeae; border-radius: 5px; padding: 10px;width:72%">
            <div style="width: 100%">
                <table style=" width:100%">
                    <tr style="width: 100%">
                        <td style="width: 10%;"></td>
                        <td style="width: 10%;" class="alnR">
                            <label id="lblDateFrom" for="txtDateFrom" class="ng-cloak" ng-bind="getLabel('lblDateFrom')"></label>
                        </td>
                        <td style="width: 15%">
                            <input type="text" id="txtDateFrom" style="width: 100%;" readonly />
                        </td>
                        <td style="width: 10%;" class="alnR">
                            <label id="lblDateTo" for="txtDateTo" class="ng-cloak" ng-bind="getLabel('lblDateTo')"></label>
                        </td>
                        <td style="width: 15%">
                            <input type="text" id="txtDateTo" style="width: 100%;" readonly />
                        </td>
                        <td style="width: 10%;" class="alnR">
                            <label id="lblSeqNo" for="txtSeqNo" class="ng-cloak" ng-bind="getLabel('lblSeqNo')"></label>
                        </td>
                        <td style="width: 20%">
                            <input type="text" id="txtSeqNo" style="width: 100%;" readonly />
                        </td>
                        <td style="width: 10%;"></td>
                    </tr>
                   
                </table>
            </div>
        </div>
        <div class="row" id="1stDiv" style="border: 1px solid #b2aeae; border-radius: 5px; padding: 10px;  margin: 1.6% 14% auto">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div>
                    <table style="width: 100%">
                        <thead style="text-align: center">
                            <tr style="color: #29449b; width: 100%">
                                <td style="width:20%"></td>
                                <td style="width:15%"><span id="lblUnit" style="text-align: center;" ng-bind="getLabel('lblUnit')"></span></td>
                                <td style="width:15%"><span id="lblDonatDate" style="text-align: center;" ng-bind="getLabel('lblDonatDate')"></span></td>
                                <td style="width:15%"><span id="lblSegmentNo" style="text-align: center;" ng-bind="getLabel('lblSegmentNo')"></span></td>
                                <td style="width:15%"><span id="lblBloodGroup" style="text-align: center;" ng-bind="getLabel('lblBloodGroup')"></span></td>
                                <td style="width:20%"></td>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="obj in obj.demo" style="width: 100%">
                                <td style="width:20%"></td>
                                <td style="width: 15%">
                                    <input type="text" style="width: 100%" class="form-control" readonly />
                                </td>
                                <td style="width: 15%">
                                    <input type="text" style="width: 100%" class="form-control" readonly />
                                </td>
                                <td style="width: 15%">
                                    <input type="text" style="width: 100%" class="form-control" readonly />
                                </td>
                                <td style="width: 15%">
                                    <input type="text" style="width: 100%" class="form-control" readonly />
                                </td>
                                <td style="width:20%"></td>

                            </tr>
                        </tbody>
                        <tbody>
                            <tr style="width:100%">
                                <td style="width:20%"></td>
                                <td style="width:15%;text-align:right">
                                    <span id="lblEntryUser" ng-bind="getLabel('lblEntryUser')"></span>
                                </td>
                                <td style="width: 30%" colspan="2">
                                    <input type="text" style="width: 100%" class="form-control" readonly />
                                </td>

                                <td style="width:35%" colspan="2"></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

