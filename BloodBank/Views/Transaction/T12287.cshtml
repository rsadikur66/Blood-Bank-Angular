
@{
    ViewBag.Title = "T12287";
}
<script src="~/Apps/JSController/Transaction/T12287Controller.js"></script>
<script src="~/Apps/JSService/Transaction/T12287Service.js"></script>


@section container
{
    <div id="options" ng-app="BloodBank" ng-controller="T12287Controller">
        <button id="btnNew" title="New" class="Button New"></button>
        <button id="btnSave" title="Save" class="Button Save" ng-disabled="obj.myDiv.$invalid"></button>
        <button id="btnNext" title="Next" class="Button Next"></button>
        <button id="btnDelete" title="Delete" class="Button Delete"></button>
        <button id="btnClear" title="Clear" class="Button Clear"></button>
        <button id="btnEnterQuery" title="Enter Query" class="Button Enter"></button>
        <button id="btnExecuteQuery" title="Execute Query" class="Button Execute"></button>
    </div>
}
    <div class="form-horizontal" ng-app="BloodBank" ng-controller="T12287Controller" name="obj.myDiv" novalidate ng-form>
        <div class="row" style="margin: 1.6% auto; border: 1px solid #b2aeae; border-radius: 5px; padding: 10px;width:72%">
            <div style="width: 100%">
                <table style=" width:100%">
                    <tr style="width: 100%;">
                        <td style="width: 9%;" class="alnR">
                            <label id="lblDateG" for="txtDateG" class="ng-cloak" ng-bind="getLabel('lblDateG')"></label>
                        </td>
                        <td style="width: 9%">
                            <input type="text" id="txtDateG" style="width: 100%;" readonly />
                        </td>
                        <td style="width: 9%;" class="alnR">
                            <label id="lblDateH" for="txtDateH" class="ng-cloak" ng-bind="getLabel('lblDateH')"></label>
                        </td>
                        <td style="width: 9%">
                            <input type="text" id="txtDateH" style="width: 100%;" readonly />
                        </td>
                        <td style="width: 9%;" class="alnR">
                            <label id="lblLocation" for="txtLocation" class="ng-cloak" ng-bind="getLabel('lblLocation')"></label>
                        </td>
                        <td style="width: 9%">
                            <input type="text" id="txtLocation" style="width: 100%;" readonly />
                        </td>
                        <td style="width: 28%">
                            <input type="text" id="txtLocationDesc" style="width: 100%;" readonly />
                        </td>
                        <td style="width: 9%;" class="alnR">
                            <label id="lblShift" for="txtShift" class="ng-cloak" ng-bind="getLabel('lblShift')"></label>
                        </td>
                        <td style="width: 9%">
                            <input type="text" id="txtShift" style="width: 100%;" readonly />
                        </td>

                    </tr>

                </table>
            </div>
        </div>
        <div class="row" id="1stDiv" style="border: 1px solid #b2aeae; border-radius: 5px; padding: 10px;  margin: 1.6% 14% auto">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div>
                    <table style="width: 100%">
                        <thead style="text-align: center">

                            <tr style="color: #29449b; width: 100%">
                                <td style="width:50%" colspan="2"><span id="lblIssues" ng-bind="getLabel('lblIssues')"></span></td>
                                <td style="width:50%" colspan="2"><span id="lblReply" ng-bind="getLabel('lblReply')"></span></td>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="obj in obj.demo" style="width: 100%">
                                <td style="width: 50%" colspan="2">
                                    <textarea style="width: 100%;height:80px" class="form-control" readonly></textarea>
                                </td>
                                <td style="width: 50%" colspan="2">
                                    <textarea style="width: 100%;height:80px" class="form-control" readonly></textarea>
                                </td>

                            </tr>
                        </tbody>
                        <tfoot>
                            <tr style="width: 100%;height:50px">
                                <td style="width: 15%" class="alnL">
                                    <label id="lblEntry" for="txtShift" class="ng-cloak" ng-bind="getLabel('lblEntry')"></label>
                                </td>
                                <td style="width: 35%" class="alnL">
                                    <label id="lblReq" for="txtReq" class="ng-cloak" ng-bind="getLabel('lblReq')"></label>
                                </td>
                                <td style="width: 15%" class="alnL">
                                    <label id="lblUpd" for="txtUpd" class="ng-cloak" ng-bind="getLabel('lblUpd')"></label>
                                </td>
                                <td style="width: 35%" class="alnL">
                                    <label id="lblUpdBy" for="txtUpdBy" class="ng-cloak" ng-bind="getLabel('lblUpdBy')"></label>
                                </td>

                            </tr>
                            <tr style="width: 100%">
                                <td style="width: 15%">
                                    <input type="text" id="txtShift" style="width: 100%;" readonly />
                                </td>
                                <td style="width: 35%">
                                    <input type="text" id="txtReq" style="width: 100%;" readonly />
                                </td>
                                <td style="width: 15%">
                                    <input type="text" id="txtUpd" style="width: 100%;" readonly />
                                </td>
                                <td style="width: 35%">
                                    <input type="text" id="txtUpdBy" style="width: 100%;" readonly />
                                </td>

                            </tr>
                        </tfoot>

                    </table>
                </div>
            </div>
        </div>
    </div>

